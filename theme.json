{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"border": {
			"radius": false,
			"color": false,
			"style": false,
			"width": false
		},
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "contrast"
				},
				{
					"color": "#4aa1de",
					"name": "Celestial Blue",
					"slug": "tertiary"
				},
				{
					"color": "#07137B",
					"name": "Phthalo Blue",
					"slug": "primary"
				},
				{
					"color": "#FADF4B",
					"name": "Naples Yellow",
					"slug": "secondary"
				},
				{
					"color": "#5a331c",
					"name": "Seal Brown",
					"slug": "accent-brown"
				},
				{
					"color": "#92bc2b",
					"name": "Apple Green",
					"slug": "accent-green"
				}
			],
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"duotone": [],
			"gradients": [],
			"link": false,
			"text": true,
			"background": true,
			"defaultGradients": true,
			"defaultPalette": true
		},
		"custom": {},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1000px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"customFontSize": true,
			"lineHeight": false,
			"dropCap": true,
			"fluid": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 400,
							"src": ["file:./assets/fonts/roboto-v30-latin/roboto-v30-latin-regular.woff2"]
						},
						{
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": 400,
							"src": ["file:./assets/fonts/roboto-v30-latin/roboto-v30-latin-italic.woff2"]
						},
						{
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 700,
							"src": ["file:./assets/fonts/roboto-v30-latin/roboto-v30-latin-700.woff2"]
						},
						{
							"fontFamily": "Roboto",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": 700,
							"src": ["file:./assets/fonts/roboto-v30-latin/roboto-v30-latin-700italic.woff2"]
						}
					],
					"fontFamily": "Roboto",
					"name": "Roboto",
					"slug": "roboto"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "10rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "blue"
		},

		"typography": {}
	}
}
